# Backend Server Port (Optional, defaults to 3000)
# PORT=3000

# VAPI Configuration
VAPI_API_KEY=YOUR_VAPI_API_KEY_HERE
VAPI_PHONE_NUMBER_ID=YOUR_VAPI_PHONE_NUMBER_ID_HERE # ID of the phone number registered in VAPI for outbound calls

# Twilio Configuration (Often used by VAPI for outbound calls)
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_1=+15551110001 # Example, replace with your actual Twilio numbers if used directly or by VAPI
TWILIO_PHONE_2=+15551110002
TWILIO_PHONE_3=+15551110003
TWILIO_PHONE_4=+15551110004
TWILIO_PHONE_5=+15551110005
TWILIO_PHONE_6=+15551110006
TWILIO_PHONE_7=+15551110007
TWILIO_PHONE_8=+15551110008
TWILIO_PHONE_9=+15551110009
TWILIO_PHONE_10=+15551110010

# Pabbly Connect Configuration
PABBLY_WEBHOOK_URL=YOUR_PABBLY_WEBHOOK_URL_HERE

# Go High Level Configuration
GHL_API_KEY=YOUR_GHL_API_KEY_HERE

# Google Sheets Configuration
# Path to your Google Cloud service account key file
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account-key.json
# The ID of the Google Sheet containing client data
GOOGLE_SHEET_ID=YOUR_SPREADSHEET_ID_HERE
# The name of the sheet tab containing client data (e.g., 'Sheet1', 'Potential Clients')
GOOGLE_SHEET_NAME=YOUR_SHEET_NAME_HERE
# The range within the sheet containing client data (e.g., 'A2:E' for columns A to E starting from row 2)
GOOGLE_SHEET_RANGE=YOUR_SHEET_RANGE_HERE